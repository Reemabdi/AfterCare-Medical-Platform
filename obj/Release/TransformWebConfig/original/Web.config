	<!-- ************ single system.web block (merged) ************ -->
	<system.web>
		<customErrors mode="Off" />
		<compilation debug="true" targetFramework="4.8">
			<assemblies>
				<!-- needed for some NuGet libraries on shared hosts -->
				<add assembly="netstandard, Version=2.0.0.0, Culture=neutral, PublicKeyToken=cc7b13ffcd2ddd51" />
				<!-- enable ZipArchive / ZipArchiveMode without adding a csproj reference -->
				<add assembly="System.IO.Compression, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
				<add assembly="System.IO.Compression.FileSystem, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
			</assemblies>
		</compilation>
		<httpRuntime targetFramework="4.8" maxRequestLength="51200" />
		<authentication mode="Forms">
			<forms loginUrl="~/Account/Login.aspx" defaultUrl="~/Home.aspx" timeout="2880" cookieless="UseCookies" />
		</authentication>
		<authorization>
			<allow users="*" />
		</authorization>
		<pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID" />
		<sessionState cookieless="UseCookies" mode="InProc" timeout="60" />
	</system.web>

	<system.webServer>
		<httpErrors errorMode="Detailed" existingResponse="PassThrough" />
		<validation validateIntegratedModeConfiguration="false" />
		<defaultDocument enabled="true">
			<files>
				<clear />
				<add value="Home.aspx" />
			</files>
		</defaultDocument>

		<staticContent>
			<!-- allow IIS to serve xlsx and zip downloads -->
			<remove fileExtension=".xlsx" />
			<mimeMap fileExtension=".xlsx" mimeType="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" />
			<remove fileExtension=".zip" />
			<mimeMap fileExtension=".zip" mimeType="application/zip" />
		</staticContent>

		<security>
			<requestFiltering>
				<requestLimits maxAllowedContentLength="52428800" />
			</requestFiltering>
		</security>
	</system.webServer>

